{
  "name": "advice-giver-backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "test": "jest --watch --verbose",
    "migrate": "knex migrate:latest --env production",
    "seed": "knex seed:run --env production",
    "api-docs": "apidoc -f .js -i ./routes -o apidoc/",
    "build-docs": "apidoc-markdown -p ./apidoc/ -o README.md",
    "build-api": "npm-run-all api-docs build-docs"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "helmet": "^3.18.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.19.5",
    "knex-cleaner": "^1.3.0",
    "nodemon": "^1.19.1",
    "npm": "^6.11.2",
    "npm-run-all": "^4.1.5",
    "pg": "^7.11.0"
  },
  "devDependencies": {
    "jest": "^24.8.0",
    "supertest": "^4.0.2"
  }
}
